package dekoratorHero;
public class Hero {
 private int HP = 50;
 private int Stamina = 100;
 private int Mana = 25;
 public int getHP() {
 return this.HP;
 }
 public int getStamina() {
 return this.Stamina;
 }
 public int getMana() {
 return this.Mana;
 }
 public void getStats() {
 System.out.println("Hero has born with stats: [" + HP + " HP/" + Stamina
+ " Stamina/" + Mana+" Mana]");
 }
}
