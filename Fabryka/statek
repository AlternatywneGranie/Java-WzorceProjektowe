package rekrutacja;
public class Statek implements IJednostkaWodna{
 private final double a;
 public Statek(double a) {
 this.a = a;
 }
 @Override
 public double poziom() {
 return (3*a) * a;
 }
 @Override
 public double predkosc() {
 return 8 * a;
 }
 @Override
 public double ladownosc() {
 return 20 * a;
 }
}
