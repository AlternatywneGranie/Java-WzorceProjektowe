package rekrutacja;
public class Samolot implements IJednostkaPowietrzna{
 private final double a;
 public Samolot(double a) {
 this.a = a;
 }
 @Override
 public double poziom() {
 return (5*a) * a;
 }
 @Override
 public double bomba() {
 return 20 * a;
 }
 @Override
 public double paliwo() {
 return 100 * a;
 }
}
